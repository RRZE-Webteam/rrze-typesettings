{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/rrze-typesettings",
  "title": "Typesettings RRZE",
  "version": "0.0.6",
  "description": "A block to show code highlightening",
  "category": "widgets",
  "icon": "admin-appearance",
  "keywords": [
    "highlight",
    "code"
  ],
  "textdomain": "rrze-typesettings",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "pre code"
    },
    "alignment": {
      "type": "string",
      "default": "none"
    }
  },
  "editorScript": "file:./index.js",
  "script": "file:./frontend.js",
  "style": "file:./style-index.css",
  "editorStyle": "file:./editor-style.css"
}