{
  "name": "rrze-typesettings",
  "version": "1.2.11",
  "description": "Plugin for displaying code and formulas",
  "author": "RRZE Webteam",
  "license": "GPL-2.0-or-later",
  "main": "build/index.min.js",
  "scripts": {
    "build-css": "sass --style=compressed src/sass/:build/",
    "build-js": "wp-scripts build src/js/index.js --output-filename build/index.min.js && wp-scripts build src/js/rrze-typesettings.js --output-filename build/rrze-typesettings.min.js",
    "watch-css": "sass --watch src/sass/:build/",
    "watch-js-index": "wp-scripts watch src/js/index.js",
    "watch-js-rrze": "wp-scripts watch src/js/rrze-typesettings.js",
    "watch": "npm-run-all --parallel watch-css watch-js-index watch-js-rrze",
    "increment-version": "node increment-version.js patch",
    "build": "npm run increment-version && npm-run-all --parallel build-css build-js",
    "start": "npm run watch",
    "lint:js": "wp-scripts lint-js",
    "lint:css": "wp-scripts lint-style 'src/**/*.scss'",
    "format:js": "wp-scripts format",
    "format:css": "wp-scripts format 'src/**/*.scss'"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.0.2",
    "sass": "^1.64.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "prismjs": "^1.29.0"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}