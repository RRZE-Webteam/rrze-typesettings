{
  "name": "rrze-typesettings",
  "version": "1.2.8",
  "description": "Plugin for displaying code and formulas",
  "author": "RRZE Webteam",
  "license": "GPL-2.0-or-later",
  "main": "build/js/index.min.js",
  "scripts": {
    "increment-version": "node increment-version.js patch",
    "build": "npm run lint && npm run build:css && npm run build:js && npm run increment-version",
    "build:css": "sass src/scss/:build/css/ --style compressed",
    "build:js": "wp-scripts build --output-path=build/js --output-filename=rrze-typesettings.min.js",
    "format": "wp-scripts format",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "start": "npm-run-all --parallel start:*",
    "start:js": "wp-scripts start --output-path=build/js --output-filename=rrze-typesettings.js",
    "start:css": "sass --watch src/scss/:build/css/",
    "test": "npm run test:e2e && npm run test:unit",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.3.0",
    "eslint": "^9.13.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-recommended": "^14.0.1",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.1.2",
    "sass": "^1.43.4",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "prismjs": "^1.29.0"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}